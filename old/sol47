perl -e '@p=(2,3,5,7,11);sub isp{return if$_[0]==1;for$p(@p){return 1 if $_[0] == $p;return if $_[0]%$p==0}push @p,$_[0];return 1}sub pf{$t=$_[0];my@r=();for(@p){while($t%$_==0){$t/=$_;push@r,$_ if$r[$#r]!=$_}last if $t==1}return\@r}sub cf{%t=();return unless @{$_[0]} == @{$_[1]} && @{$_[0]} == 4;$t{$_}++ for map{@$_} @_;return scalar grep {$_!=1} values %t == 0}for(1..500000){isp(2*$_)};for($n=2;$n<1000000000;$n++){isp($n);print "t:$n:$d\n" if $n % 1000000 == 0; if (@{pf($n)} == 4) {$c++;$d++} else {$c=0} if ($c>$mc) {$mc=$c;print "CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCc:$c:$n\n"}}'
