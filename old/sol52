perl -e '@ch=(2,3,4,5,6);for$t(100000..166666){@m=();for$c(@ch){$n=$c*$t;if($n=~/^[$t]+$/&&$t=~/^[$n]+$/){push@m,$n}else{last}}if(@m==@ch){%d=();$d{$_}++for(split//,$t);@v=();for$m(@m){my%t=();$t{$_}++for split//,$m;push@v,\%t}$n=0;for$c(@v){$m=0;for$d(keys%d){$m++if$c->{$d}==$d{$d}}$n++if$m==keys%d}if($n==@ch){print"$t @m $n\n"}}}'
