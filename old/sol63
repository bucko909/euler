perl -e 'sub bigf{for$v(0..$#{$_[0]}){if($_[0][$v]>=1000){$o=int($_[0][$v]/1000);$_[0][$v+1]+=$o;$_[0][$v]%=1000}}}sub bigm{$n=[map{$_[1]*$_}@{$_[0]}];bigf($n);return$n}sub biga{$l=@{$_[0]}>@{$_[1]}?@{$_[0]}:@{$_[1]};my$n=[];for(0..$l-1){$n->[$_]=$_[0][$_]+$_[1][$_]}bigf($n);return$n}for$b(1..10){for$e(1..100){$a=[1];for(1..$e){$a=bigm($a,$b);last if$#$a>3*$e}$l=0;$l=length($a->[$#$a])+$#$a*3;print"$b $e $l (@$a)\n" if $l == $e;}}'|wc -l
