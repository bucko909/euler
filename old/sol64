perl -e 'sub h{return h(h($_[0],$_[1]),@_[2..$#_])if@_>2;return$_[0]if@_==1;my$a=$_[0]>$_[1]?$_[0]:$_[1];my$b=$_[0]>$_[1]?$_[1]:$_[0];while($b){my$bn=$a%$b;$a=$b;$b=$bn}return$a}sub l{my$n=$_[0];my$a=int(sqrt($n));my$b=$n-$a*$a;my$t=1;my$s=$a;my%c=();for(1..4000){print"$a+($t*r($n)+$s)/$b\n"if 0;return 0 if$b==0;my$an=int(($t*sqrt($n)+$s)/$b);return 0 if$an**2==($t*sqrt($n)+$s)/$b;my$tn=$t*$b;my$sn=$an*$b*$b-$s*$b;my$bn=$t*$t*$n-($s-$an*$b)**2;$a=$an;my$h=h($bn,$sn,$tn);$b=$bn/$h;$s=$sn/$h;$t=$tn/$h;if(exists$c{"$t $s $b"}){return$_-$c{"$t $s $b"}}$c{"$t $s $b"}=$_}}for(2..10000){print"$_\n"if l($_)%2}'|wc -l
