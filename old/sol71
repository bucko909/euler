primes 1 1000000|perl -e 'while(<>){chomp;push@p,$_;$p{$_}=1}@f=(5000);$m=2;while(1){$n=1;$n*=$p[$_]for@f;if($n>=1000000/2){for($i=$#f;$i>0;$i--){$f[$i]=++$f[$i-1];$n=1;$n*=$p[$_]for@f;last if$n<1000000*3/7}if($i==0){push@f,0;for($i=0;$p[$i]**@f<100000;$i++){}@f=($i)x@f;last if$i==0&&2**@f>1000000/2}next}print"$n @f @p[@f]\n";$f[$#f]++}'
